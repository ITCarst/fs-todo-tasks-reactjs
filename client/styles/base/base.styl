@import "../helpers/variables"
@import "../helpers/mixins"

html, body, * {
	margin: 0;
	padding: 0;
	font-family: Arial, sans-serif;
	font-size: 14px;
    color: #848f99;
}

*, *:before, *:after { 
	box-sizing: border-box; 
}

html, body
	width 100%
	height 100%


#main 
	height 100%
	
ul 
	list-style none
	margin 0
	padding 0

.green 
	color green